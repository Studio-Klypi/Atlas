{
  "name": "atlas",
  "version": "0.0.0",
  "author": "Loic Maes",
  "description": "Internal ERP for Studio Klypi",
  "type": "module",
  "private": true,
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx ./prisma/seed.ts"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "add:component": "tsx ./scripts/shadcn-add.ts",
    "add:module": "tsx ./scripts/nuxt-add-module.ts",
    "lint": "eslint . --fix",
    "db:migrate": "prisma migrate dev --name",
    "db:create-migration": "prisma migrate dev --create-only --name",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@nuxt/eslint": "1.9.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.11.13",
